.mine-photo {
  height: 310rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}
.mine-photo-img {
  width: 140rpx;
  height: 140rpx;
  border-radius: 70rpx;
  background-color: #fa6;
}
.mine-photo-nickname {
  width: 140rpx;
  line-height: 78rpx;
  font-size: 28rpx;
  text-align: center;
}

.mine-content-icon {
  width: 65rpx;
  height: 33rpx;
}
.mine-content-icon-img {
  width: 33rpx;
  height: 33rpx;
}

.message-frame-style {
  height: 100rpx;
}