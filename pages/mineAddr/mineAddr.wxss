page {
  width: 750rpx;
  background-color: #eee;
  padding: 0;
}
.mineAddr {
  background-color: #fff;
  padding: 26rpx;
}
.mineAddr-telName text{
  font-size: 24rpx;
  color: #999;
}
.mineAddr-address {
  margin-top: 25rpx;
}
.mineAddr-address-default {
  background-color: #FFE300;
  width: 77rpx;
  line-height: 30rpx;
  border-radius: 15rpx;
  text-align: center;
  float: left;
  margin-right: 20rpx;
}
.mineAddr-btn {
  color: #999;
}
.message-frame-style {
  height: 144rpx;
}

.mineAddr-create {
  margin: 40rpx auto;
  width: 646rpx;
  height: 100rpx;
  border-radius: 12rpx;
  background-color: #f44;
  font-size: 34rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.mineAddr-create text{
  color: #fff;
  font-weight: 500;
}
.mineAddr-create-icon {
  margin-right: 20rpx;
  width: 34rpx;
  line-height: 34rpx;
  font-size: 34rpx;
  border-radius: 4rpx;
  background-color: #fff;
  color: #f44;
  text-align: center;
  font-weight: 500;
}
